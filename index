    <style>
        .extension-slide-in {
            transform: translateX(calc(150px * var(--slide-direction, 0)));
        }

        .extension-slide-out {
            transform: translateX(calc(-150px * var(--slide-direction, 0)));
        }
    </style>
    <script>
        const KEY_POSITIONS = {
            'smore-for': 1,
            'templates': 2,
            'remote-learning': 3,
            'pricing': 4,
            'help': 5,
            'my-smore': 6,
        };


        function toolbar() {
            return {
                isOpenDesktop: false,
                isOpenMobile: false,
                timeout: undefined,
                key: "",
                lastKey: "",
                directionStyle: "",
                mainMobile: "slogan",
                open(key) {
                    if (this.timeout) {
                        window.clearTimeout(this.timeout);
                    }
                    this.timeout = window.setTimeout(() => {
                        this.lastKey = this.key;
                        this.isOpenMobile = false;
                        this.isOpenDesktop = true;
                        this.key = key;
                        this.directionStyle = '--slide-direction: ' + this.direction(key)
                    }, 150)

                },
                closeDesktop() {
                    if (this.timeout) {
                        window.clearTimeout(this.timeout);
                    }
                    this.directionStyle = "";
                    this.isOpenDesktop = false;
                    if (!this.isOpenMobile) {
                        this.key = "";
                    }
                },
                close() {
                    if (this.timeout) {
                        window.clearTimeout(this.timeout);
                    }
                    this.directionStyle = "";
                    this.isOpenDesktop = false;
                    this.isOpenMobile = false;
                    this.key = "";
                },
                openMobile(key) {
                    this.isOpenDesktop = false;
                    this.isOpenMobile = true;
                    this.key = key;
                },
                closeMobile() {
                    this.isOpenMobile = false;
                    this.key = "";
                },
                direction(key) {
                    if (!this.lastKey) {
                        return 0;
                    }

                    if (KEY_POSITIONS[this.lastKey] > KEY_POSITIONS[this.key]) {
                        return -1;
                    }

                    return 1;
                }
            }
        }

        // setup scroll

        let last_known_scroll_position = 0;
        let ticking = false;

        function onScroll(scroll_pos) {
            if (scroll_pos > 30) {
                document.body.classList.remove("scroll-top")
            } else {
                document.body.classList.add("scroll-top")
            }

            // Make sure all the heights are correct
            setTimeout(updateToolbarPlaceholderHeight, 100)
        }

        window.addEventListener('scroll', function(e) {
            last_known_scroll_position = window.scrollY;

            if (!ticking) {
                window.requestAnimationFrame(function() {
                    onScroll(last_known_scroll_position);
                    ticking = false;
                });

                ticking = true;
            }
        });

        function updateToolbarPlaceholderHeight() {
            var allBarsHeight = document.querySelector('.bars-container').offsetHeight;
            document.querySelector('.toolbar-placeholder').style.height = allBarsHeight + "px";
            document.querySelector('.toolbar-extension').style.top = allBarsHeight + "px";
            window.__fullToolbarHeight = allBarsHeight;
        }
    </script>

